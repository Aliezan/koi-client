{
  "extends": [
    "next/core-web-vitals",
    "plugin:prettier/recommended",
    "next/typescript",
    "next"
  ],
  "plugins": ["prettier"],
  "rules": {
    "@typescript-eslint/ban-ts-comment": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
